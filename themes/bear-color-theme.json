	"name": "theme-bear",
	"tokenColors": [{
		"settings": {
			"foreground": "#bcb28d",
			"background": "#424852"
		}
	}, {
		"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#595855"
			}
	}, {
		"name": "Storage Type",
			"scope": "keyword.control.import.js, keyword.control.from.js, storage.type.function.arrow.js, storage.type.class.js, keyword.operator.expression.typeof.js, keyword.control.trycatch.js, keyword.control.loop.js, keyword.control.switch.js, keyword.operator.new.js, storage.type.js, storage.type.function.js,  keyword.control.conditional.js, keyword.control.flow.js",
			"settings": {
				"foreground": "#da7748"
			}
	}, {
		"name": "Entity Name Function",
			"scope": "entity.name.function.js, entity.name.type.js, support.class.builtin.js, support.variable.property.js, entity.name.type.class.js",
			"settings": {
				"foreground": "#22A7F0"
			}
	}, {
		"name": "Support Variable Dom",
			"scope": "support.variable.dom.js",
			"settings": {
				"foreground": "#22A7F0"
			}
	}, {
		"name": "Support Function",
			"scope": "support.function.js, support.function.dom.js",
			"settings": {
				"foreground": "#22A7F0"
			}
	}, {
		"name": "Variable Parameter",
			"scope": "variable.parameter.js",
			"settings": {
				"foreground": "#A2ABA5"
			}
	}, {
		"name": "Variable Parameter",
			"scope": "variable.language.this.js, variable.other.readwrite.alias.js",
			"settings": {
				"foreground": "#A2ABA5"
			}
	}
	]
